{
  "name": "objectdocs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "objectdocs dev",
    "build": "objectdocs build",
    "build:site": "pnpm --filter '@objectdocs/site' -r build",
    "start": "pnpm --filter '@objectdocs/site' -r start",
    "translate": "objectdocs translate",
    "translate:all": "objectdocs translate --all",
    "test": "pnpm -r test",
    "test:site": "bash test-site.sh",
    "test:quick": "bash test-quick.sh",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "dependencies": {
    "client-only": "^0.0.1",
    "lucide-react": "^0.564.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@objectdocs/cli": "workspace:*"
  }
}
